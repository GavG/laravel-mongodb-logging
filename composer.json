  "version": "1.0.0",
  "keywords": [
    "laravel",
    "logging",
    "log",
    "database",
    "db",
    "mongodb"
  ],
  "authors": [
    {
      "name": "SergiiNechaienko",
      "email": "sergii.nechaenko@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "~5.6",
    "jenssegers/mongodb": "~3.4"
  },
  "autoload": {
    "psr-4": {
      "Nechaienko\\MongodbLogging\\": "src"
    }
  }
}
