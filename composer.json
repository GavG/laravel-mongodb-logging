{
  "name": "nechaienko/laravel-mongodb-logging",
  "version": "1.0.0",
  "description": "Custom Larvel 5.6+ Log channel handler that can store log events to MongoDB databases. Uses Laravel native logging functionality.",
  "keywords": [
    "laravel",
    "logging",
    "log",
    "database",
    "db",
    "mongodb"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Sergii Nechaienko",
      "email": "sergii.nechaenko@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "~5.6",
    "nechaienko/laravel-mongodb-logging": "dev-master"
  },
  "minimum-stability": "dev",
  "require-dev": {
    "orchestra/testbench": "~3.6",
    "jenssegers/mongodb": "~3.4"
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "*"
      ]
    }
  }
}
